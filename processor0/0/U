FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    arch        "LSB;label=64;scalar=64";
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0.9);

boundaryField
{
    inletCH4
    {
        type            fixedValue;
        value           uniform (0 0 49.6);
    }
    wallOutside
    {
        type            zeroGradient;
    }
    wallTube
    {
        type            noSlip;
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform (0 0 11.4);
    }
    inletAir
    {
        type            fixedValue;
        value           uniform (0 0 0.9);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0.9);
    }
    axis
    {
        type            empty;
    }
    frontAndBack_pos
    {
        type            wedge;
    }
    frontAndBack_neg
    {
        type            wedge;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0.9);
    }
}


// ************************************************************************* //
